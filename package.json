{
  "name": "js-dev-environment",
  "version": "1.0.0",
  "description": "Minimal, Yet Useful JavaScript Dev Environment",
  "main": "index.js",
  "scripts": {
    "test": "nyc ava",
    "lint": "standard",
    "dev": "chokidar '**/*.js' -c 'standard && nyc ava'",
    "report": "nyc report --reporter=html",
    "validate": "npm ls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cameronrye/js-dev-environment.git"
  },
  "keywords": [],
  "author": "Cameron Rye",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cameronrye/js-dev-environment/issues"
  },
  "homepage": "https://github.com/cameronrye/js-dev-environment#readme",
  "pre-commit": [
    "lint",
    "test"
  ],
  "devDependencies": {
    "ava": "latest",
    "chokidar-cli": "latest",
    "coveralls": "latest",
    "nyc": "latest",
    "precommit-hook": "latest",
    "standard": "latest"
  }
}